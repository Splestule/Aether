# Server Configuration
PORT=8080
NODE_ENV=development

# API Configuration
OPENSKY_API_URL=https://opensky-network.org/api/states/all
OPENSKY_TRACKS_API_URL=https://opensky-network.org/api/tracks/all
ELEVATION_API_URL=https://api.open-elevation.com/api/v1/lookup
OPENSKY_CLIENT_ID=
OPENSKY_CLIENT_SECRET=
OPENSKY_AUTH_URL=https://auth.opensky-network.org/auth/realms/opensky-network/protocol/openid-connect/token

# Cache Configuration
CACHE_TTL=300
CACHE_CHECK_PERIOD=60

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
CORS_ORIGIN=http://localhost:3000,http://localhost:5173

# Logging
LOG_LEVEL=info

# Bring Your Own Key (BYOK)
# When set to true, allows users to provide their own OpenSky credentials
# Users without credentials will have limited API access (10 requests/minute)
# Users with credentials will have the same quota as server credentials
# When false, the app behaves exactly as before (server credentials only)
BYOK=false
